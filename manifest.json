{
    "manifest_version": 3,
    "name": "ReallyBlock",
    "version": "0.0.6",
    "description": "Fully hide messages and threads from blocked Discord users",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "js": ["scripts/index.js"],
            "matches": ["https://discord.com/*"]
        }
    ]
}